package travianFarmList;

import java.util.ArrayList;

public class ReadSource
{
	ArrayList<Point> points = new ArrayList<Point>();
	
	public ReadSource(String source)
	{
		boolean stillTrue = true;
		
		source = source.substring(source.indexOf("<span class='koord'>"),source.indexOf("</table></div"));
		while(stillTrue)
		{
			int x, y, placeX, placeY;
			
			placeX = source.indexOf("</span></span>");
			placeY = source.indexOf("( <span>");
			x = Integer.parseInt(source.substring(placeY + 8,placeX));
			System.out.print(x);
			stillTrue = false;
		}
	}
}
